import java.util.*;
import java.util.LinkedList;
class SumList 
{
	public static void main(String[] args) 
	{
		int sum=0;
		
		LinkedList<Integer> list=new LinkedList<>();
		list.add(10);
		list.add(20);
		list.add(30);
		list.add(40);
		list.add(50);
		list.add(60);
		System.out.println(list);
		for(int num:list)
		{
			sum+=num;
		}
		System.out.println(sum);
		
	}
}
