import java.util.*;
import java.util.LinkedList;
class RemoveList
{
public static void main(String[]args)
{
LinkedList<Integer> list=new LinkedList<>();
list.add(10);
list.add(20);
list.add(30);
list.add(40);
list.add(50);
list.add(60);
System.out.println(list);
list.remove(1);
System.out.println(list);
list.remove(Integer.valueOf(30));
System.out.println(list);
list.clear();
System.out.println(list);
}
}
